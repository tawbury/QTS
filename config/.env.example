###########################################################################
# QTS Environment Variables - Example Template
# 
# 복사하여 .env 파일로 사용 (gitignore 대상)
# 이 파일은 시크릿 키와 비밀번호의 템플릿입니다.
###########################################################################

###########################################################################
# Broker Credentials (KIWOOM VTS - 모의투자)
###########################################################################
KIWOOM_MODE=KIWOOM_VTS

KIWOOM_VTS_APP_KEY=your_vts_app_key_here
KIWOOM_VTS_APP_SECRET=your_vts_app_secret_here
KIWOOM_VTS_ACCOUNT_NO=your_vts_account_number_here
KIWOOM_VTS_ACNT_PRDT_CD=01
KIWOOM_VTS_BASE_URL=https://openapi.koreainvestment.com:9443

###########################################################################
# Broker Credentials (KIWOOM REAL - 실전투자)
###########################################################################
# KIWOOM_MODE=KIWOOM_REAL
# KIWOOM_REAL_APP_KEY=your_real_app_key_here
# KIWOOM_REAL_APP_SECRET=your_real_app_secret_here
# KIWOOM_REAL_ACCOUNT_NO=your_real_account_number_here
# KIWOOM_REAL_ACNT_PRDT_CD=01
# KIWOOM_REAL_BASE_URL=https://openapi.koreainvestment.com:9443

###########################################################################
# Database Credentials
###########################################################################
DB_USER=qts
DB_PASSWORD=your_db_password_here

###########################################################################
# Google Sheets API (Optional)
###########################################################################
GOOGLE_SHEET_KEY=your_google_sheet_id_here
GOOGLE_CREDENTIALS_FILE=./config/schema/google_credentials.json

###########################################################################
# Real Order Safety Switch
###########################################################################
ENABLE_REAL_ORDER=N
